{
  "manifest_version": 3,
  "name": "TabFabric",
  "version": "1.0.0",
  "description": "Color and group your tabs for better focus and control.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon-128.png"
    }
  },
  "permissions": [
    "tabs",
    "tabGroups",
    "activeTab"
  ],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "128": "icons/icon-128.png"
  }
}
